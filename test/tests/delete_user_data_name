source tests/test_env
TEST_NAME="API: /users/<user>/data/<name> (DELETE)"
TEST_CASE="delete_user_data_name"
TEST_DESC="Delete user data records using data name"

curl -s -f -S\
     -H "Authorization: Bearer $TKN"\
     -d @$USER_DATA\
     -X DELETE\
     http://$FGAPISERVER_HOST/fgapiserver/$FGAPISERVER_APIVER/users/futuregateway/data/TEST_DATA_NAME
RES=$?
exit $RES
